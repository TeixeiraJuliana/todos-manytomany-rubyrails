<p id="notice"><%= notice %></p>
<div class="main">
  <div class="container">
  <h1>Themes</h1>

  <table>
    <thead>
      <tr>
        <th>Title list</th>
        <th>Description list</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @themes.each do |theme| %>
        <tr>
          <td><%= theme.title_list %></td>
          <td><%= theme.description_list %></td>
          <td><%= link_to 'Show', theme %></td>
          <td><%= link_to 'Edit', edit_theme_path(theme) %></td>
          <td><%= link_to 'Destroy', theme, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Theme', new_theme_path %>
  </div>
</div>